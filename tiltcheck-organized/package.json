{
  "name": "traphouse-discord-bot",
  "version": "2.0.0",
  "description": "TrapHouse Discord Bot with TiltCheck Integration",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "start:all": "./start_bot.sh",
    "start:routing": "node routing.js",
    "start:tiltcheck": "./start_tiltcheck_main.sh",
    "start:beta": "node beta-testing-server.js",
    "start:github": "node github-webhook-server.js",
    "start:collectclock": "node collectClockOAuthHandler.js",
    "start:degens": "node degens_bot.js",
    "test": "node crypto-test.js",
    "test:kofi": "node test-kofi-webhook.js",
    "test:endpoints": "./test_tiltcheck_endpoints.sh",
    "test:support": "node test-support-page.js",
    "health": "curl http://localhost:4001/health && curl http://localhost:3333/health",
    "status": "ps aux | grep node",
    "stop": "pkill -f 'node.*\\.js'",
    "dev": "nodemon bot.js",
    "dev:routing": "nodemon routing.js",
    "logs": "tail -f *.log",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.6.1",
    "ethers": "^5.8.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "fs-extra": "^11.2.0",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.7",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^5.8.3",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "stripe": "^18.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.2",
    "web3": "^4.16.0",
    "ws": "^8.16.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "hardhat": "^3.0.7",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "discord",
    "bot",
    "tiltcheck",
    "traphouse",
    "degen",
    "mindful",
    "gambling",
    "collectclock",
    "justthetip",
    "railway"
  ],
  "author": "jmenichole",
  "license": "PROPRIETARY",
  "homepage": "https://jmenichole.github.io/TiltCheck/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmenichole/TiltCheck.git"
  },
  "bugs": {
    "url": "https://github.com/jmenichole/traphouse_discordbot/issues"
  }
}
